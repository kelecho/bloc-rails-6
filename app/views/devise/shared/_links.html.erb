<%- if controller_name != 'sessions' %>
  <%= link_to "Ya tengo mi cuenta", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registrate", new_registration_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Olvidaste tu contrasenha?", new_password_path(resource_name) %><br />
<% end %>

